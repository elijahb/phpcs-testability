{
    "name": "elijahb/phpcs-testability",
    "description": "PHPCodeSniffer sniffs for unit test code testability check",
    "homepage": "https://github.com/elijahb/phpcs-testability",
    "keywords": ["phpcs", "sniff", "php-code-sniffer", "phpunit"],
    "require": {
        "squizlabs/php_codesniffer": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Testability\\": "src/Testability"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Testability\\Tests\\": "tests"
        }
    }
}
